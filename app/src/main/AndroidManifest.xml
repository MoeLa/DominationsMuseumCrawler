<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bhg.sucks.so.we.need.a.dominationsmuseumcrawler">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_dmc"
        android:roundIcon="@mipmap/ic_launcher_round_dmc"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">

        <service
            android:name="bhg.sucks.service.OverlayIconService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name="bhg.sucks.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="bhg.sucks.activity.CreateKeepRuleActivity"
            android:parentActivityName="bhg.sucks.activity.MainActivity" />
    </application>

</manifest>